<?xml version="1.0" encoding="UTF-8"?>

<Application xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PSPDFKit.Maui.Catalog.App">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/Colors.xaml" />
                <ResourceDictionary Source="Resources/Styles/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>

            <x:String x:Key="AndroidLicenseKey">
                <!--Paste your MAUI (Android) license key with `com.pspdfkit.maui.windows` bundle id here-->
            </x:String>
            <x:String x:Key="iOSLicenseKey">
                <!--Paste your MAUI (iOS) license key with `com.pspdfkit.maui.ios` bundle id here-->
            </x:String>
            <x:String x:Key="MacCatalystLicenseKey">
                <!--Paste your MAUI (Mac Catalyst) license key with `com.pspdfkit.maui.macos` bundle id here-->
            </x:String>
            <x:String x:Key="WindowsLicenseKey">
                <!--Paste your MAUI (Windows) license key with `com.pspdfkit.maui.windows` bundle id here-->
            </x:String>
        </ResourceDictionary>
    </Application.Resources>
</Application>
